<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx/8.0.65">
    <TitledPane text="Attributes" contentDisplay="RIGHT" prefWidth="200.0">
        <graphic>
            <Button mnemonicParsing="false" text="Add"/>
        </graphic>
        <ListView fx:id="attributes"/>
    </TitledPane>
    <ScrollPane HBox.hgrow="ALWAYS" fx:id="attributePane"/>
</HBox>
